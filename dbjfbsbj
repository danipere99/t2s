const Sequelize = require('sequelize')
const sequelize = new Sequelize ('test', 'root', 'root3306', {
    host: "localhost",
    dialect: 'mysql'
})

const Usuarios = sequelize.define('usuario',{
    cliente: {
        type: Sequelize.STRING
    },
    numeracao: {
        type: Sequelize.TEXT
    }
})

//Usuarios.sync({force: true})


